{
  "extends": [
    "next",
    "prettier",
    "next/core-web-vitals",
    "plugin:storybook/recommended"
  ],
  "plugins": ["unused-imports"],
  "rules": {
    "no-console": "error",
    "no-unused-vars": "off",
    "unused-imports/no-unused-imports": "error",
    "react/no-unescaped-entities": "off"
  }
}
